    "rxjs": "^6.3.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/cheerio": "^0.22.11",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.13",
    "@types/jsonstream": "^0.8.30",
    "@types/kue": "^0.11.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "^10.12.18",
    "@types/node-cron": "^2.0.2",
    "@types/oboe": "^2.0.28",
    "@types/request-promise-native": "^1.0.15",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^3.4.4",
    "jest": "^24.7.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.12.1",
    "typescript": "^3.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
