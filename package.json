    "brain.js": "^1.6.1",
    "cheerio": "^1.0.0-rc.2",
    "date-fns": "^1.30.1",
    "express": "^4.17.3",
    "giantbomb": "^2.0.2",
    "igdb-api-node": "^3.1.7",
    "kue": "^0.11.6",
    "lodash": "^4.17.11",
    "node-cron": "^2.0.3",
    "oboe": "^2.1.4",
